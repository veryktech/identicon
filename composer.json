{
    "name": "veryktech/identicon",
    "type": "library",
    "description": "Create awesome unique avatar.",
    "keywords": [
        "avatar",
        "identicon",
        "image"
    ],
    "homepage": "https://github.com/veryktech/identicon",
    "license": "MIT",
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "ext-imagick": "*",
        "fzaninotto/faker": "^1.2.0",
        "phpunit/phpunit": "^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Identicon\\": "src/Identicon/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Identicon\\Tests\\": "tests/Identicon/Tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
